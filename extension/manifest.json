{
    "manifest_version": 3,
    "name": "YouTube Video Duplicator",
    "version": "1.0",
    "description": "Creates a new instance of the current YouTube video and displays it on the page.",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
      "*://*/*"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  